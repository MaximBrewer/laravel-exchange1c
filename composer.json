{
    "name": "maximbrewer/laravel-exchange1c",
    "type": "library",
    "description": "Catalog Loader from 1c - CommerceML with laravel",
    "keywords": [
      "1c",
      "CommerceML",
      "Laravel"
    ],
    "homepage": "https://github.com/maximbrewer",
    "license": "MIT",
    "authors": [
      {
        "name": "Anton Kartsev",
        "role": "Developer"
      }
    ],
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": {
        "MaximBrewer\\LaravelExchange1C\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "require": {
      "php": ">=7.4",
      "laravel/framework": "^5.4|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^7",
      "orchestra/testbench": "~3.0",
      "mockery/mockery": "^1.2"
    },
    "extra": {
      "laravel": {
        "providers": [
          "MaximBrewer\\LaravelExchange1C\\Exchange1CServiceProvider"
        ]
      }
    }
  }
  